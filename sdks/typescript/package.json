{
  "name": "opensandbox",
  "version": "0.4.0",
  "description": "TypeScript SDK for OpenSandbox - open-source cloud sandbox platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sandbox",
    "opensandbox",
    "microvm",
    "cloud",
    "containers",
    "code-execution"
  ],
  "author": "OpenSandbox",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diggerhq/opensandbox.git",
    "directory": "sdks/typescript"
  },
  "homepage": "https://github.com/diggerhq/opensandbox/tree/main/sdks/typescript",
  "bugs": {
    "url": "https://github.com/diggerhq/opensandbox/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  }
}
